   "source": [
    "from Bio import SeqIO\n",
    "\n",
    "with open('HE681097.fasta') as in_f, open('HE681097.bed','w') as out_f:\n",
    "    for record in SeqIO.parse(in_f, 'fasta'):\n",
    "        out_f.write('{}\\t0\\t{}\\n'.format(record.id, len(record)))"
   ]
  }
